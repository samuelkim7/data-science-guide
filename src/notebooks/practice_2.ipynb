   "source": [
    "n = 0\n",
    "star = \"\"\n",
    "i = 0\n",
    "while n < 5:\n",
    "    while(i <= n):\n",
    "        star += \"*\"\n",
    "        i = i+1\n",
    "    star += \"\\n\"\n",
    "    i = 0\n",
    "    n = n+1\n",
    "print(star)\n",
